issn = {1900-8260},
journal = {Revista Educaci{\'{o}}n en Ingenier{\'{i}}a},
keywords = {Aprendizaje Colaborativo Apoyado por Computador,Dise{\~{n}}o de Actividades Colaborativas},
month = {jun},
number = {17},
pages = {143--149},
title = {{DISE{\~{N}}ANDO ACTIVIDADES DE APRENDIZAJE COLABORATIVO ASISTIDAS POR COMPUTADOR}},
