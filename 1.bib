keywords = {CS2,Curriculum Design,Survey},
pages = {10 -- 13},
title = {{What Do “CS1” and “CS2” Mean? Investigating Differences In the Early Courses}},
url = {http://ims.mii.lt/ims/konferenciju{\_}medziaga/SIGCSE{\%}2710/docs/p199.pdf},
year = {2010}
}
