@article{Ruiz-Rivas2016,
abstract = {La docencia reglada en temas de cooperaci{\'{o}}n dentro de nuestras propias universidades es un aspecto central de la Cooperaci{\'{o}}n Universitaria para el Desarrollo. En el presente trabajo se recoge la experiencia desarrollada por el Grupo de Tecnolog{\'{i}}as Apropiadas de la Universidad Carlos III de Madrid, tras seis a{\~{n}}os impartiendo asignaturas de libre elecci{\'{o}}n en el tema y dos a{\~{n}}os m{\'{a}}s impartiendo dos asignaturas optativas en los grados adaptados a Bolonia y abiertas a un amplio abanico de titulaciones. La oferta actual se plantea como una introducci{\'{o}}n sobre la realidad del subdesarrollo, la cooperaci{\'{o}}n y las pautas de adecuaci{\'{o}}n tecnol{\'{o}}gica que deben seguirse, todo ello realizado desde un enfoque pr{\'{a}}ctico. Se presenta una discusi{\'{o}}n sobre las motivaciones y los objetivos y sobre los contenidos y la metodolog{\'{i}}a que debieran incluirse para una visi{\'{o}}n global y aplicada del problema. Finalmente se presenta el programa y organizaci{\'{o}}n de las asignaturas que se ofertan actualmente. L{\'{i}}nea Tem{\'{a}}tica: 2. La actividad universitaria como pr{\'{a}}ctica de cooperaci{\'{o}}n al desarrollo INTRODUCCI{\'{O}}N -JUSTIFICACI{\'{O}}N La necesidad de incluir asignaturas que traten aspectos del subdesarrollo y la cooperaci{\'{o}}n dentro de los planes de estudio universitarios aparece reflejada en m{\'{u}}ltiples documentos. El C{\'{o}}digo de Conducta de las Universidades en Materia de Cooperaci{\'{o}}n al Desarrollo establece como prioridades de acci{\'{o}}n de la universidad la docencia y la investigaci{\'{o}}n, a las que define como el {\'{a}}mbito natural de actuaci{\'{o}}n de la misma. En el art{\'{i}}culo 19 se especifica claramente que " las Universidades estudiar{\'{a}}n las f{\'{o}}rmulas necesarias para el reconocimiento acad{\'{e}}mico de las actividades propias de la cooperaci{\'{o}}n al desarrollo ({\ldots}) favoreciendo la implantaci{\'{o}}n de asignaturas... " Al mismo tiempo, el proceso de Bolonia, actualmente en implantaci{\'{o}}n en las universidades espa{\~{n}}olas, incluye dentro de sus objetivos program{\'{a}}ticos, desde los primeros documentos hasta la actualidad, la introducci{\'{o}}n de conceptos de solidaridad y cooperaci{\'{o}}n para el desarrollo en los planes de estudio. Es evidente que la docencia es uno de los {\'{a}}mbitos b{\'{a}}sicos de la actividad universitaria. Sin embargo, y pese al volumen alcanzado por las acciones de Cooperaci{\'{o}}n Universitaria para el Desarrollo (CUD), no se ha llegado a una estabilizaci{\'{o}}n razonable en este aspecto. Aunque toda la legislaci{\'{o}}n sobre planes de estudios propone su inserci{\'{o}}n de forma transversal en los estudios, esta inclusi{\'{o}}n, dentro de las asignaturas " normales " de una titulaci{\'{o}}n, plantea diversos problemas, en especial los de implicaci{\'{o}}n real del profesorado y la necesidad de una formaci{\'{o}}n previa y especializada del mismo. Por ello, una soluci{\'{o}}n alternativa es la de la elaboraci{\'{o}}n de asignaturas vinculadas a los planes de estudio y centradas en estos temas. Los objetivos de estas asignaturas deben ser tanto de sensibilizaci{\'{o}}n y divulgaci{\'{o}}n como formativos y, en {\'{u}}ltimo punto, t{\'{e}}cnicos, recogiendo as{\'{i}} todo el espectro de motivaciones, intereses y formas de acercamiento al tema del alumnado.},
author = {Ruiz-Rivas, Ulpiano and Mart{\'{i}}nez-Crespo, Jorge and Bueno, Miriam and Chinchilla, M{\'{o}}nica},
file = {:Users/giovanny/Library/Application Support/Mendeley Desktop/Downloaded/Ruiz-Rivas et al. - 2016 - Una asignatura de tecnolog{\'{i}}as apropiadas y cooperaci{\'{o}}n para el desarrollo en el grado.pdf:pdf},
title = {{Una asignatura de tecnolog{\'{i}}as apropiadas y cooperaci{\'{o}}n para el desarrollo en el grado}},
url = {https://www.researchgate.net/profile/Jorge{\_}Martinez-Crespo/publication/266289329{\_}Una{\_}asignatura{\_}de{\_}tecnologias{\_}apropiadas{\_}y{\_}cooperacion{\_}para{\_}el{\_}desarrollo{\_}en{\_}el{\_}grado/links/54b3e08e0cf2318f0f9694f3/Una-asignatura-de-tecnologias-apropiadas-y-cooperacion-pa},
year = {2016}
}
