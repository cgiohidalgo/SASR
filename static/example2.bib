file = {:Users/giovanny/Library/Application Support/Mendeley Desktop/Downloaded/Rosanigo, Paur - Unknown - Estrategias para la ense{\~{n}}anza de Algor{\'{i}}tmica y Programaci{\'{o}}n.pdf:pdf},
title = {{Estrategias para la ense{\~{n}}anza de Algor{\'{i}}tmica y Programaci{\'{o}}n}},
