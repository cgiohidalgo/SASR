file = {:Users/giovanny/Library/Application Support/Mendeley Desktop/Downloaded/S{\'{e}}in-Echaluce et al. - Unknown - A Knowledge Management System to Classify Social Educational Resources Within a Subject Using Teamwork.pdf:pdf},
